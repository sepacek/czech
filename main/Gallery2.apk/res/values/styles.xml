<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EffectIcon">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">@dimen/effect_icon_size</item>
        <item name="android:layout_height">@dimen/effect_icon_size</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="EffectLabel">
        <item name="android:textSize">@dimen/effect_label_text_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/effect_label_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/effect_label_margin_top</item>
        <item name="android:maxLines">1</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
    <style name="Effect">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">@dimen/effect_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/effect_padding_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EffectsContainer">
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">@dimen/effects_container_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EffectsBar">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="EffectLabelInToolPanel" parent="@style/EffectLabel">
        <item name="android:layout_marginTop">0.0dip</item>
    </style>
    <style name="SeekBar">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:paddingLeft">@dimen/seekbar_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/seekbar_padding_vertical</item>
        <item name="android:paddingRight">@dimen/seekbar_padding_horizontal</item>
        <item name="android:paddingBottom">@dimen/seekbar_padding_vertical</item>
        <item name="android:layout_width">@dimen/seekbar_width</item>
        <item name="android:layout_height">@dimen/seekbar_height</item>
        <item name="android:maxHeight">@dimen/seekbar_height</item>
        <item name="android:minHeight">@dimen/seekbar_height</item>
    </style>
    <style name="ActionBarInner">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">?android:actionBarSize</item>
    </style>
    <style name="ActionBarOuter">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ActionBarLinearLayout">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="ActionBarBackLinearLayout" parent="@style/ActionBarLinearLayout">
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="ActionBarImageView">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="ActionBarArrow">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/action_bar_arrow_margin_left</item>
        <item name="android:layout_marginRight">@dimen/action_bar_arrow_margin_right</item>
        <item name="android:src">?android:homeAsUpIndicator</item>
    </style>
    <style name="ActionBarIcon" parent="@style/ActionBarImageView">
        <item name="android:paddingLeft">@dimen/action_bar_icon_padding_left</item>
        <item name="android:paddingTop">@dimen/action_bar_icon_padding_vertical</item>
        <item name="android:paddingRight">@dimen/action_bar_icon_padding_right</item>
        <item name="android:paddingBottom">@dimen/action_bar_icon_padding_vertical</item>
        <item name="android:src">@mipmap/ic_launcher_gallery</item>
    </style>
    <style name="ActionBarTitle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="ImageActionButton" parent="@style/ActionBarImageView">
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingLeft">@dimen/action_button_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/action_button_padding_vertical</item>
        <item name="android:paddingRight">@dimen/action_button_padding_horizontal</item>
        <item name="android:paddingBottom">@dimen/action_button_padding_vertical</item>
    </style>
    <style name="TextActionButton" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Menu">
        <item name="android:gravity">center</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingLeft">@dimen/action_button_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/action_button_padding_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="EffectsMenuContainer" parent="@style/ActionBarLinearLayout">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/effects_menu_container_width</item>
    </style>
    <style name="EffectsMenuActionButton" parent="@style/ImageActionButton">
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FullscreenToolView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="SpinnerProgressDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="Theme.Gallery" parent="@android:style/Theme.Holo">
        <item name="android:colorBackground">@null</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:actionBarStyle">@style/Holo.ActionBar</item>
        <item name="android:displayOptions">none</item>
    </style>
    <style name="Transparent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="ActionBar.Tab.Style" parent="@com.nubia:style/Widget.ActionBar.TabView">
        <item name="android:background">@drawable/improve_tab_indicator</item>
        <item name="android:minWidth">155.0dip</item>
        <item name="android:paddingStart">10.0dip</item>
        <item name="android:paddingEnd">10.0dip</item>
    </style>
    <style name="ActionBar.Style" parent="@com.nubia:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:backgroundStacked">@drawable/improve_tab_bg</item>
    </style>
    <style name="ActionBar.TabBar.Style" parent="@com.nubia:style/Widget.Holo.ActionBar.TabBar">
        <item name="android:divider">@null</item>
    </style>
    <style name="ActionBar.Titile.Style" parent="@com.nubia:style/Widget.Holo.ActionBar.TabText">
        <item name="android:textSize">@com.nubia:dimen/nubia_res_action_bar_title_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@drawable/tab_color_selector</item>
    </style>
    <style name="GalleryThemeDark" parent="@com.nubia:style/Theme.Nubia">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarStyle">@style/ActionBar.Style</item>
        <item name="android:actionBarTabStyle">@style/ActionBar.Tab.Style</item>
        <item name="android:actionBarTabBarStyle">@style/ActionBar.TabBar.Style</item>
        <item name="android:actionBarTabTextStyle">@style/ActionBar.Titile.Style</item>
        <item name="baceground">@drawable/baceground</item>
        <item name="improve_slideshow_shortcuts">@drawable/improve_slideshow_shortcuts</item>
        <item name="improve_actionbar_bg">@drawable/improve_actionbar_bg</item>
        <item name="improve_jigsaw">@drawable/improve_jigsaw</item>
        <item name="improve_gif">@drawable/improve_gif_white</item>
        <item name="improve_move">@drawable/improve_move_white</item>
        <item name="improve_private">@drawable/improve_private</item>
        <item name="improve_map_actionalbum">@drawable/improve_map_actionalbum</item>
        <item name="improve_map_actionmap">@drawable/improve_map_actionmap</item>
        <item name="improve_mark_marked">@drawable/improve_mark_marked</item>
        <item name="improve_mark_unmark">@drawable/improve_mark_unmark</item>
        <item name="ic_menu_camera_holo_light">@drawable/ic_menu_camera_holo_light</item>
        <item name="ic_menu_share_holo_light">@drawable/ic_menu_share_holo_light</item>
        <item name="ic_menu_slideshow_holo_light">@drawable/ic_menu_slideshow_holo_light</item>
        <item name="ic_menu_trash_holo_light">@drawable/ic_menu_trash_holo_light</item>
        <item name="improve_btn_rotate_leftrotate">@drawable/improve_btn_rotate_leftrotate</item>
        <item name="improve_btn_rotate_rightrotate">@drawable/improve_btn_rotate_rightrotate</item>
        <item name="improve_btn_rotate_freerotate">@drawable/improve_btn_rotate_freerotate</item>
        <item name="video_app_icon">@drawable/video_app_icon</item>
        <item name="footer_color">@android:color/black</item>
        <item name="enabled_color">@android:color/white</item>
        <item name="disabled_color">@color/gray_white</item>
    </style>
    <style name="GalleryCrop" parent="@style/GalleryThemeDark">
        <item name="android:actionMenuTextColor">@android:color/black</item>
    </style>
    <style name="Holo.ActionBar" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/actionbar_translucent</item>
        <item name="android:displayOptions">useLogo|showHome</item>
        <item name="android:backgroundStacked">@null</item>
    </style>
    <style name="DialogPickerTheme" parent="@style/Theme.Gallery" />
    <style name="WidgetTypeChooserTheme" parent="@style/Theme.Gallery">
        <item name="android:windowFullscreen">false</item>
        <item name="android:actionBarStyle">@null</item>
    </style>
    <style name="imporve_Slideshow_EffectIcon">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@drawable/photoeditor_toggle_button_background</item>
        <item name="android:layout_width">@dimen/effect_icon_size</item>
        <item name="android:layout_height">@dimen/effect_icon_size</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="imporve_Effect">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="improve_EffectLabel" parent="@style/EffectLabel">
        <item name="android:layout_width">@dimen/effect_icon_size_slideshow</item>
    </style>
    <style name="Theme.MovieFullscreenLight" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarStyle">@style/Holo.ActionBar</item>
    </style>
    <style name="Theme.MovieFullscreenDark" parent="@android:style/Theme.DeviceDefault">
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarStyle">@style/Holo.ActionBar</item>
    </style>
    <style name="ImproveImageIcon">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">@dimen/improve_image_icon_width</item>
        <item name="android:layout_height">@dimen/improve_image_icon_height</item>
        <item name="android:layout_weight">1.0</item>
    </style>
</resources>
