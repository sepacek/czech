<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="FilterShowHistoryButton">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:padding">2.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FilterShowTopButton">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:padding">8.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="FilterShowBottomButton">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:paddingBottom">16.0dip</item>
        <item name="android:layout_width">96.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="FilterShowSlider">
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">13.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/filtershow_slider</item>
        <item name="android:progressDrawable">@drawable/filtershow_slider</item>
        <item name="android:minHeight">13.0dip</item>
        <item name="android:thumb">@drawable/filtershow_scrubber</item>
        <item name="android:thumbOffset">16.0dip</item>
        <item name="android:paddingStart">16.0dip</item>
        <item name="android:paddingEnd">16.0dip</item>
    </style>
    <style name="FilterIconButton">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textSize">@dimen/small_text_size</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/filtershow_button_background</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingTop">4.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
        <item name="android:paddingBottom">4.0dip</item>
        <item name="android:layout_width">@dimen/thumbnail_width</item>
        <item name="android:layout_height">@dimen/thumbnail_height</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">3.0dip</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>
    <style name="AppBaseTheme" parent="@android:style/Theme.Holo.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme">
        <item name="android:colorBackground">@null</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:actionBarStyle">@style/Holo.Light.ActionBar</item>
        <item name="android:displayOptions">none</item>
    </style>
    <style name="Holo.Light.ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/actionbar_translucent</item>
        <item name="android:displayOptions">useLogo|showHome</item>
        <item name="android:backgroundStacked">@null</item>
    </style>
    <style name="ActionBar.Style" parent="@com.nubia:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:backgroundStacked">@drawable/improve_tab_bg</item>
    </style>
    <style name="ActionBar.Tab.Style" parent="@com.nubia:style/Widget.ActionBar.TabView">
        <item name="android:background">@drawable/improve_tab_indicator</item>
        <item name="android:minWidth">64.0dip</item>
        <item name="android:paddingStart">10.0dip</item>
        <item name="android:paddingEnd">10.0dip</item>
    </style>
    <style name="ActionBar.TabBar.Style" parent="@com.nubia:style/Widget.Holo.ActionBar.TabBar">
        <item name="android:divider">@null</item>
    </style>
    <style name="PhotoEditorTheme" parent="@com.nubia:style/Theme.Nubia">
        <item name="android:windowFullscreen">true</item>
        <item name="android:actionBarStyle">@style/ActionBar.Style</item>
        <item name="android:actionBarTabStyle">@style/ActionBar.Tab.Style</item>
        <item name="android:actionBarTabBarStyle">@style/ActionBar.TabBar.Style</item>
    </style>
    <style name="CursorSeekBar" parent="@android:style/Widget.Holo.Light.SeekBar">
        <item name="android:maxHeight">100.0dip</item>
        <item name="android:progressDrawable">@drawable/seek_bar_progress</item>
        <item name="android:thumb">@drawable/seek_bar_thumb</item>
        <item name="android:thumbOffset">0.0dip</item>
        <item name="android:paddingStart">0.0dip</item>
        <item name="android:paddingEnd">0.0dip</item>
    </style>
    <style name="tabbar_btn_item">
        <item name="android:textSize">@dimen/tab_text_size</item>
        <item name="android:textColor">@drawable/btn_enable_disable</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/tabbar_bg</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
    </style>
</resources>
